#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
  int i=1,j,N,k;
    cin>>N;
    while(i<=N)
    {
        j=N-i+1;
        k=1;
        while(k<=N-i+1)
        {
            cout<<j;
            k=k+1;
        }
        cout<<endl;
        i=i+1;
        
    }
    
}
