#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
 
    int c,y,i=0,j=1,N;
    cin>>N;
    
    for(y=1;y<=N;y++)
    {
        c=i+j;
        i=j;
		j=c;
	
     if(y==N)
        cout<<i<<endl;
}
}


