#include<iostream>
using namespace std;


int main(){
    
    /*  Read input as specified in the question.
     * Print output as specified in the question.
     */
    int i=1,j,N;
    cin>>N;
    while(i<=N)
    {
        int j=1;
        while(j<=i)
        {
            char ch='A'+i-1;
        cout<<ch;    
           j=j+1; 
        }
        cout<<endl;
        i=i+1;
   }
    
}

