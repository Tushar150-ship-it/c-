#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
int i=1,j,val=1,N;
    cin>>N;
    
    while(i<=N)
    {
        j=1;
        char start='A'+i-1;
        while(j<=i)
        {
            cout<<start;
            j++;
            start++;
        }
        cout<<endl;
        i++;
    }
}
