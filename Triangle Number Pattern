#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
    int i=1,j=1,N;
    cin>>N;
    while(i<=N)
    {
        j=1;
        while(j<=i)
        {
            cout<<i;
            j=j+1;
        }
        i=i+1;
        cout<<endl;
    }
}


