#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
 int i=1,j,N,p;
    cin>>N;
    while(i<=N)
    {
      j=1;
        p=i;
        while(j<=i)
        {
         cout<<p;
            p=p-1;
            j=j+1;
        }
        i=i+1;
        cout<<endl;
    }
}
